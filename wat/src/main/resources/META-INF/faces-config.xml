<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <managed-bean>
        <managed-bean-name>autoCompleteController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.mapping.wat.controller.AutoCompleteController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>notificationsController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.mapping.wat.controller.NotificationsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>mdslRegistryController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.mapping.wat.controller.MDSLRegistryController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>mdslrRequestController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.mapping.wat.controller.MDSLRRequestController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>sessionMDSLRRequestController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.mapping.wat.controller.SessionMDSLRRequestController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
